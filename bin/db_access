#!/usr/bin/env python
# -*- coding: utf-8

import argparse

from tRNAsorter import db_accessor

if __name__ == '__main__':
    parser = argparse.ArgumentParser(description="Access tRNA database")
    parser.add_argument("db_path", help="path of the tRNA database")

    args = parser.parse_args()
   
    db_accessor.DB_Accessor(args.db_path).run()

